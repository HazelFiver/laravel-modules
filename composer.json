    "description": "a custom laravel modules based on nwidart/laravel-modules",
    "type": "tools",
    "require": {
        "nwidart/laravel-modules": "^4.0",
        "php": "^7.1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wanghouting",
            "email": "wanghouting@163.com"
        }
    ],
    "autoload": {
